<%= render 'books/part', book: @book, user: current_user %>
<div class="col-xs-9">
<h2>Users</h2>
<table class="table table-hovertable-inverse" >
<thead>
<tr>
<th></th>
<th>Image</th>
<th>Name</th>
<th colspan="3"></th>
</tr>
</thead>

<% @users.each do |user| %>
<tr>
<td><td><%= link_to user_path(user.id) do %>
	<%= attachment_image_tag user, :profile_image, :fill, 50, 50, format: 'jpeg', fallback: "no_image.jpg",size:"50x50"%>
	<% end %></td>
<td><%= user.name %></td>
<td><%= link_to "SHOW",user_path(user.id) %></td>



<% end %>
</table>
